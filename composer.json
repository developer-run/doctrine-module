{
  "name": "devrun/doctrine-module",
  "description": "DevRun Doctrine module to create database",
  "keywords": ["cms", "devrun", "database", "doctrine"],
  "homepage": "http://devrun.cz",
  "license": "proprietary",
  "version": "v3.3.1",
  "authors": [
    {
      "name": "Pavel Paul√≠k"
    }
  ],
  "require": {
    "php":"^7.0",
    "kdyby/doctrine": "^3.3",
    "symfony/validator": "^3.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/developer-run/doctrine-module"
    }
  ],
  "autoload": {
    "classmap": ["src/"]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.3-dev"
    },
    "devrun": {
      "configuration": {
        "extensions": {
          "doctrine": "Devrun\\DoctrineModule\\DI\\DoctrineExtension"
        }
      }
    }
  }
}
